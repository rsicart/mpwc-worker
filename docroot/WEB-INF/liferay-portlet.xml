<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	
	<portlet>
		<portlet-name>workerportlet</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/vader/jquery-ui-1.9.0.custom.css</header-portlet-css>
		<header-portlet-css>/css/ui.multiselect.css</header-portlet-css>
		<header-portlet-css>/css/ui.jqgrid.css</header-portlet-css>
		<header-portlet-javascript>
			/js/jquery-1.8.2.js
		</header-portlet-javascript>
		<header-portlet-javascript>
			/js/jquery-ui-1.8.2.custom.min.js
		</header-portlet-javascript>

		<header-portlet-javascript>
			/js/i18n/grid.locale-en.js
		</header-portlet-javascript>
		<header-portlet-javascript>
			/js/ui.multiselect.js
		</header-portlet-javascript>
		<header-portlet-javascript>
			/js/jquery.jqGrid.js
		</header-portlet-javascript>
		<header-portlet-javascript>
			/js/jquery.tablednd.js
		</header-portlet-javascript>
		<header-portlet-javascript>
			/js/jquery.contextmenu.js
		</header-portlet-javascript>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>workerportlet-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>